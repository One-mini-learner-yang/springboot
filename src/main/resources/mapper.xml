<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.springboot.mapperInterface">
    <cache eviction="FIFO"
            flushInterval="60000"
            size="512"
            readOnly="true"></cache>
    <select id="selectA" resultType="java.util.Map">
        select * from test
    </select>
    <select id="selectB" resultType="java.util.Map">
        select *from test
    </select>
</mapper>